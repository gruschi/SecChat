<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cs.hm.edu.sisy.chat"
    android:versionCode="1"
    android:versionName="1.0" >

   <uses-permission android:name="android.permission.INTERNET" />
   <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    
    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="21" />
    
  <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
    <activity android:name="cs.hm.edu.sisy.chat.FriendList">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"></action>
        <category
          android:name="android.intent.category.NORMAL">
        </category>
      </intent-filter>
    </activity>
    <service android:name="cs.hm.edu.sisy.chat.services.IMService"></service>
    <activity android:name="cs.hm.edu.sisy.chat.Login">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"></action>
        <category android:name="android.intent.category.LAUNCHER">
        </category>
      </intent-filter>
    </activity>
    <activity android:name="cs.hm.edu.sisy.chat.Messaging">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"></action>
        <category
          android:name="android.intent.category.NORMAL">
        </category>
      </intent-filter>
    </activity>
    
    
  <activity android:name="cs.hm.edu.sisy.chat.SignUp"><intent-filter><action android:name="android.intent.action.MAIN"></action>
<category android:name="android.intent.category.NORMAL"></category>
</intent-filter>
</activity>
<activity android:name="cs.hm.edu.sisy.chat.AddFriend"><intent-filter><action android:name="android.intent.action.MAIN"></action>
<category android:name="android.intent.category.NORMAL"></category>
</intent-filter>
</activity>
<activity android:name="cs.hm.edu.sisy.chat.UnApprovedFriendList"><intent-filter><action android:name="android.intent.action.MAIN"></action>
<category android:name="android.intent.category.NORMAL"></category>
</intent-filter>
</activity>
</application>

</manifest>
